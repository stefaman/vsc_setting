// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+e",
        "command": "cursorLineEnd"
    },
    {
        "key": "ctrl+a",
        "command": "cursorLineStart"
    },
    {
        "key": "ctrl+p",
        "command": "cursorUp",
        "when": "editorTextFocus"
    },
    {
        "key": "up",
        "command": "cursorUp",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+oem_comma",
        "command": "cursorTop",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+home",
        "command": "-cursorTop",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+oem_period",
        "command": "cursorBottom",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+n",
        "command": "cursorDown",
        "when": "editorTextFocus"
    },
    {
        "key": "down",
        "command": "cursorDown",
        "when": "editorTextFocus"
    },
]